{
  "name": "osu-server-list",
  "private": true,
  "version": "0.0.1",
  "packageManager": "bun@1.2.23",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "package:db:rebuild": "turbo -F @osu-server-list/db build",
    "db:push": "turbo db:push",
