{
  "name": "osu-server-list",
  "private": true,
  "version": "0.0.1",
  "packageManager": "bun@1.2.23",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "format": "prettier --write .",
    "packages:rebuild": "turbo --filter='./packages/*' build",
    "db:push": "turbo db:push",
    "db:generate": "turbo db:generate",
    "db:migrate": "turbo db:migrate",
    "db:studio": "turbo db:studio"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "typescript": "^5.8.2",
      "tsdown": "^0.15.5",
      "@better-fetch/fetch": "^1.1.18",
      "zod": "^4.1.12"
    }
  },
  "dependencies": {
    "@osu-server-list/db": "workspace:*"
  },
  "devDependencies": {
    "prettier": "3",
    "prettier-plugin-svelte": "3",
    "prettier-plugin-tailwindcss": "^0.7.0",
    "tsdown": "^0.15.6",
    "turbo": "^2.5.8",
    "valibot": "^1.1.0"
  }
}
