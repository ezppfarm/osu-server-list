      "packages/*"
    ],
    "catalog": {
      "typescript": "^5.8.2",
      "tsdown": "^0.15.5"
    }
  },
  "devDependencies": {
    "turbo": "^2.5.8",
    "tsdown": "^0.15.6"
  }
}